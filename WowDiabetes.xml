<Ui xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.blizzard.com/wow/ui/">
  <Frame name="WowDiabetesFrame" parent="UIParent" toplevel="true" movable="true" enableMouse="true">
    <Size x="200" y="200" />
    <Anchors>
      <Anchor point="CENTER">
        <Offset x="-456" y="199" />
      </Anchor>
    </Anchors>
    <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" edgeFile="Interface\DialogFrame\UI-DialogBox-Border" tile="true">
      <BackgroundInsets>
        <AbsInset left="11" right="12" top="12" bottom="11" />
      </BackgroundInsets>
      <TileSize>
        <AbsValue val="32" />
      </TileSize>
      <EdgeSize>
        <AbsValue val="32" />
      </EdgeSize>
    </Backdrop>
    <Layers>
      <Layer level="OVERLAY">
        <FontString name="$parentNameString" inherits="GameFontNormal" text="WoW Diabetes">
          <Size x="150" y="20" />
          <Anchors>
            <Anchor point="TOPLEFT">
              <Offset x="27" y="-22" />
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentGlucoseString" inherits="GameFontNormal" text="Glucose">
          <Size x="64" y="20" />
          <Anchors>
            <Anchor point="TOPLEFT">
              <Offset x="32" y="-48" />
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentMedsString" inherits="GameFontNormal" text="Medicine">
          <Size x="64" y="20" />
          <Anchors>
            <Anchor point="TOPLEFT">
              <Offset x="102" y="-48" />
            </Anchor>
          </Anchors>
        </FontString>
        <FontString name="$parentGlucoseLevelString" inherits="GameFontNormal" text="99mg/dL">
          <Size x="134" y="20" />
          <Anchors>
            <Anchor point="TOPLEFT">
              <Offset x="32" y="-105" />
            </Anchor>
          </Anchors>
        </FontString>
      </Layer>
    </Layers>
    <Frames>
      <Button name="$parentGlucoseButton" inherits="UIPanelButtonTemplate" text="Check">
        <Size x="64" y="23" />
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset x="32" y="-75" />
          </Anchor>
        </Anchors>
      </Button>
      <Button name="$parentMedicineButton" inherits="UIPanelButtonTemplate" text="Use">
        <Size x="64" y="23" />
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset x="102" y="-74" />
          </Anchor>
        </Anchors>
      </Button>
      <Button name="$parentCloseButton" inherits="UIPanelButtonTemplate" text="Close">
        <Size x="64" y="23" />
        <Anchors>
          <Anchor point="TOPLEFT">
            <Offset x="113" y="-159" />
          </Anchor>
        </Anchors>
        <Scripts>
          <OnClick>
            WowDiabetesCloseButton_OnClick()
          </OnClick>
        </Scripts>
      </Button>
      <StatusBar name="$parentGlucoseLevelBar" inherits="AchievementProgressBarTemplate" toplevel="true">
        <Size x="150" y="14" />
        <Anchors>
          <Anchor point="BOTTOM">
            <Offset x="2" y="55" />
          </Anchor>
        </Anchors>
        <Backdrop bgFile="" />
        <Layers>
          <Layer>
            <Texture name="$parentStatus" file="Interface\RaidFrame\Raid-Bar-Hp-Fill">
              <Size x="150" y="14" />
              <Anchors>
                <Anchor point="LEFT">
                  <Offset />
                </Anchor>
              </Anchors>
              <Color r="0" g="0.784" b="0" />
            </Texture>
          </Layer>
        </Layers>
        <Scripts>
          <OnLoad>
            self:SetMinMaxValues(0,100);
            self:SetValue(50);
            self:SetStatusBarTexture("Interface\\RaidFrame\\Raid-Bar-Hp-Fill");
            WowDiabetesGlucoseLevelString:SetText(self:GetValue() .. "mg/dL");
          </OnLoad>
          <OnValueChanged>
            WowDiabetesGlucoseLevelBar_OnValueChanged()
          </OnValueChanged>
        </Scripts>
      </StatusBar>
    </Frames>
	
    <Scripts>
      <OnLoad>
        WowDiabetes_OnLoad(self)
      </OnLoad>
	  <OnEvent>
		WowDiabetes_OnEvent(self, event, ...)
	  </OnEvent>
	  <OnClick>
		WowDiabetes_OnClickFrame()
	  </OnClick>
      <OnDragStart>
        self:StartMoving()
      </OnDragStart>
      <OnDragStop>
        self:StopMovingOrSizing()
      </OnDragStop>
    </Scripts>
  </Frame>
</Ui>